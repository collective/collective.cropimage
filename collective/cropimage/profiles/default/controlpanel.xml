<?xml version="1.0"?>
<object
  i18n:domain="collective.cropimage"
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  >

  <configlet
    action_id="crop_image_registry"
    appId="collective.cropimage"
    category="Products"
    condition_expr=""
    i18n:attributes="title"
    icon_expr="string:$portal_url/cut_icon.png"
    title="Crop Image Registry"
    url_expr="string:${portal_url}/@@crop-image-controlpanel"
    visible="True"
    >
    <permission>Manage portal</permission>
  </configlet>

</object>